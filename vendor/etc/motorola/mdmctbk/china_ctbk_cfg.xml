<xml>
    <ctbk_version>100</ctbk_version>
    <wifi_tx_pwr_mimo>
          <!-- (10 plimit)2.4G tx0,2.4G tx1,5G TX0_B1,5G TX0_B2,5G TX0_B3,5G TX0_B4,,5G TX1_B1,5G TX1_B2,5G TX1_B3,5G TX1_B4-->
        <Default  index="0">40,40,38,38,38,38,38,38,38,38</Default> <!--default, Max-->
        <State1   index="1">40,40,38,38,38,38,38,38,38,38</State1>  <!--WLAN + WWAN, No Sensor Triggered-->
        <State2   index="2">30,30,32,32,28,33,32,32,28,33</State2>  <!--HotSpot, Not on Body, Not next to Head-->
        <State3   index="3">22,22,22,20,20,20,22,20,20,20</State3>  <!--WLAN, Min Power, Max CB-->
        <State4   index="4">40,40,38,38,38,38,38,38,38,38</State4>  <!--WLAN StandAlone, Next to Head, Earpiece On-->
        <State5   index="5">40,40,38,38,38,38,38,38,38,38</State5>  <!--WLAN + WWAN, Next to Head, Earpiece On-->
        <State6   index="6">40,40,38,38,38,38,38,38,38,38</State6>  <!--WLAN StandAlone, On Body)-->
        <State7   index="7">30,30,32,32,28,33,32,32,28,33</State7>  <!--WLAN + WWAN, On Body-->
        <State8   index="8">22,22,22,20,20,20,22,20,20,20</State8>  <!--WLAN + WWAN, TX0_WLAN triggered Extremity-->
        <State9   index="9">35,35,35,33,33,33,35,33,33,33</State9>  <!--WLAN StandAlone, TX0_WLAN triggered Extremity-->
        <State10   index="10">44,44,44,44,44,44,44,44,44,44</State10> <!-- ? -->
