        <sensor type="prox_front"           index="0">Composite Proximity Sensor Wakeup</sensor>
        <sensor type="prox_back"            index="1">TMD2725 Alt Proximity Sensor Wakeup</sensor>
        <!--from back view-->
        <sensor type="cap_top_right"        index="2">Moto CapSense Ch0</sensor>
        <sensor type="cap_bottom_right"     index="3">Moto CapSense Ch2</sensor>
        <sensor type="cap_bottom_left"      index="4">Moto CapSense Ch3</sensor>
        <sensor type="cap_top_left"         index="5">Moto CapSense Ch4</sensor>
        <sensor type="flip sensor"          index="6">Flip Position  Wakeup</sensor>
    </sar_sensors>
    <!--OB5 config: 5 = 0000101b (cap_bottom_center|cap_top)-->
    <ob5_sensor_config>5</ob5_sensor_config>
